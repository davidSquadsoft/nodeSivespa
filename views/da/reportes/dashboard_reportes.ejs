        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3c773c',
        cancelButtonColor: '#d33',
        confirmButtonText: 'Si, eliminar'
        }).then((result) => {
        if (result.isConfirmed) {
        
            window.location='/da/deletereportesivespa/'+id;
          
        
        
        }
        })
            }
        </script>



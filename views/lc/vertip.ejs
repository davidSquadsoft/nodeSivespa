<%-include ('includes/header') -%>
    <!--Area de Principal-->

    <div class="container mt-lg-5 filtro_busqueda">

     
        <!--Noticias-->

        <div class="row mt-5 card-deck">

            <div class="col-lg-2"></div>
            <div class="col-lg-8">
                <h1 class="card-title"><%= tips.titulo %></h1>
                <img class="card-img-top" src="/upload/<%= tips.imagen %>" alt="Card image cap" style=" object-fit: cover; width: 720;height: 360px;">
                <div class="card-body">
                    <p class="card-text"><%= tips.texto %></p>
                    <p class="card-text"><small class="text-muted">Fecha: <%= tips.fcreacion.toISOString().slice(0,10) %></small></p>
                </div>
            </div>
            <div class="col-lg-2"></div>
    



           
        </div>
    </div>
    <%-include ('includes/footer') -%>